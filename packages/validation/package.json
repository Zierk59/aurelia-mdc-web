{
  "name": "@aurelia-mdc-web/validation",
  "version": "11.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Validation for Aurelia Material Components Web",
  "keywords": [
    "aurelia",
    "material",
    "components",
    "web",
    "validation"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-mdc-web/issues"
  },
  "license": "MIT",
  "author": "Maxim Balaganskiy <m.balaganskiy@gmail.com>",
  "contributors": [],
  "main": "dist/esnext/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurelia-ui-toolkits/aurelia-mdc-web"
  },
  "files": [
    "dist",
    "*.scss"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "gulp --gulpfile ../../gulpfile.js --cwd ./ build",
    "publish:dev": "npm publish --tag dev",
    "doc": "cross-env mkdirp doc && typedoc --json doc/api.json --excludeExternals --name aurelia-mdc-web-docs src/",
    "update-versions": "ts-node -P ../../tsconfig.scripts.json ../../scripts/update-versions.ts"
  },
  "dependencies": {
    "@aurelia-mdc-web/base": "11.0.0",
    "@aurelia/validation": "^2.1.0-dev.202106282211",
    "@aurelia/validation-html": "^2.1.0-dev.202106282211"
  }
}